
<div class="reportSection tableContainer">
  <table data-paging="10">
  <h4>Offshore Habitats</h4>
    <thead>
      <tr>
        <th style="width:250px;">Habitat</th>
        <th>Area (sq. km.)</th>
        <th>Area (% of Total)</th>
      </tr>
    </thead>
    <tbody>
      {{#offshore_habitats}}
      <tr>
        <td>{{HAB_TYPE}}</td>
        <td>{{AREA_SQKM}}</td>
        <td>{{PERC}}</td>
      </tr>
      {{/offshore_habitats}}
    </tbody>
  </table>
</div>
 